{
  "name": "@data-wings/sdk",
  "version": "0.1.0",
  "description": "Data Wings JavaScript/TypeScript SDK for event tracking and analytics",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MARUCIE/data-wings.git",
    "directory": "packages/sdk"
  },
  "keywords": [
    "analytics",
    "tracking",
    "data-wings",
    "event-tracking"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
