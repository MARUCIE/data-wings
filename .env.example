# Data Wings Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# LLM Configuration (priority: gemini > poe > deepseek > qwen > openai)
# =============================================================================

# Provider selection: gemini, poe, deepseek, qwen, openai
DW_LLM_PROVIDER=gemini

# Google Gemini (AI Studio) - Priority 1: $300 free credit
DW_GEMINI_API_KEY=AIzaSy...
DW_GEMINI_MODEL=gemini-2.0-flash

# Poe API (OpenAI-compatible) - Priority 2: $50-100/month
DW_POE_API_KEY=xxx
DW_POE_MODEL=GPT-4o

# DeepSeek - Priority 3 (cost-effective)
DW_DEEPSEEK_API_KEY=sk-xxx
DW_DEEPSEEK_MODEL=deepseek-chat

# Qwen / DashScope - Priority 4 (good Chinese support)
DW_QWEN_API_KEY=sk-xxx
DW_QWEN_MODEL=qwen-plus

# OpenAI - Priority 5 (optional, highest quality)
DW_OPENAI_API_KEY=sk-xxx
DW_OPENAI_MODEL=gpt-4-turbo-preview

# =============================================================================
# Database Configuration
# =============================================================================

# ClickHouse
DW_CLICKHOUSE_HOST=localhost
DW_CLICKHOUSE_PORT=8123
DW_CLICKHOUSE_USER=default
DW_CLICKHOUSE_PASSWORD=
DW_CLICKHOUSE_DATABASE=data_wings

# Redis
DW_REDIS_URL=redis://localhost:6379/0

# =============================================================================
# Service URLs (for local development)
# =============================================================================

# API Server
API_URL=http://localhost:8080

# AI Service
AI_URL=http://localhost:8000

# Web App
WEB_URL=http://localhost:3000

# =============================================================================
# Optional: Analytics & Monitoring
# =============================================================================

# Sentry (error tracking)
# SENTRY_DSN=https://xxx@sentry.io/xxx

# PostHog (product analytics - dogfooding!)
# POSTHOG_KEY=phc_xxx
# POSTHOG_HOST=https://app.posthog.com
